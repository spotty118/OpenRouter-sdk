{
  "name": "openrouter-sdk",
  "version": "1.0.6",
  "description": "A comprehensive TypeScript SDK for seamlessly integrating with the OpenRouter API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node dist/api/index.js",
    "dev": "ts-node src/api/index.ts",
    "api": "nodemon --watch src --ext ts --exec ts-node src/api/index.ts",
    "prepare": "npm run build",
    "security-audit": "npm audit",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "openrouter",
    "ai",
    "llm",
    "api",
    "sdk",
    "typescript",
    "claude",
    "gpt",
    "gemini"
  ],
  "author": "OpenRouter",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openrouter-ai/openrouter-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/openrouter-ai/openrouter-sdk/issues"
  },
  "homepage": "https://github.com/openrouter-ai/openrouter-sdk#readme",
  "dependencies": {
    "abort-controller": "3.0.0",
    "body-parser": "1.20.3",
    "chromadb": "1.5.3",
    "compression": "1.8.0",
    "cors": "2.8.5",
    "express": "4.18.2",
    "express-rate-limit": "7.1.5",
    "express-validator": "7.0.1",
    "helmet": "7.1.0",
    "hpp": "0.2.3",
    "joi": "17.11.0",
    "multer": "1.4.5-lts.1",
    "node-fetch": "3.3.2",
    "nodemon": "3.0.3",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.5",
    "@types/compression": "1.7.5",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/express-rate-limit": "6.0.0",
    "@types/helmet": "4.0.0",
    "@types/hpp": "0.2.5",
    "@types/jest": "29.5.11",
    "@types/joi": "17.2.2",
    "@types/multer": "1.4.11",
    "@types/node": "20.10.6",
    "@types/node-fetch": "2.6.10",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.6",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@typescript-eslint/parser": "6.16.0",
    "eslint": "8.56.0",
    "jest": "29.7.0",
    "prettier": "3.1.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
