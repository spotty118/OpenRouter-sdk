{
    "name": "openrouter-sdk",
    "version": "1.0.6",
    "description": "A comprehensive TypeScript SDK for seamlessly integrating with the OpenRouter API",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "test": "jest",
        "lint": "eslint src --ext .ts",
        "format": "prettier --write \"src/**/*.ts\"",
        "start": "node dist/api/index.js",
        "dev": "ts-node src/api/index.ts",
        "api": "nodemon --watch src --ext ts --exec ts-node src/api/index.ts",
        "prepare": "npm run build"
    },
    "keywords": [
        "openrouter",
        "ai",
        "llm",
        "api",
        "sdk",
        "typescript",
        "claude",
        "gpt",
        "gemini"
    ],
    "author": "OpenRouter",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/openrouter-ai/openrouter-sdk.git"
    },
    "bugs": {
        "url": "https://github.com/openrouter-ai/openrouter-sdk/issues"
    },
    "homepage": "https://github.com/openrouter-ai/openrouter-sdk#readme",
    "devDependencies": {
        "@types/body-parser": "^1.19.5",
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/helmet": "^0.0.48",
        "@types/jest": "^29.5.0",
        "@types/joi": "^17.2.2",
        "@types/multer": "^1.4.12",
        "@types/node": "^20.0.0",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.8",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.45.0",
        "jest": "^29.5.0",
        "prettier": "^3.0.0",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "abort-controller": "^3.0.0",
        "body-parser": "^1.20.3",
        "chromadb": "^1.5.3",
        "compression": "^1.8.0",
        "cors": "^2.8.5",
        "express": "^4.21.2",
        "helmet": "^7.2.0",
        "joi": "^17.13.3",
        "multer": "^1.4.5-lts.1",
        "node-fetch": "^3.3.0",
        "nodemon": "^3.0.1",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
